{
  "name": "createjs-cache-util",
  "version": "0.0.1",
  "main": "./bin/createjs-text-cache.js",
  "types": "./bin/createjs-text-cache.d.ts",
  "repository": "https://MasatoMakino@github.com/MasatoMakino/createjs-cache-util.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "description": "Bitmap Cache Utility for Createjs.text",
  "keywords": [
    "createjs"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/easeljs": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "easeljs": "^1.0.2",
    "husky": "^1.1.0",
    "prettier": "^1.14.3",
    "replace-in-file": "^3.4.2",
    "typescript": "^3.1.1",
    "typedoc": "^0.13.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "build": "npm run prettier:src && tsc && npm run babel && npm run doc",
    "prettier:src": "prettier --write './!(bin)/**/*.{js,ts,json}' ",
    "babel": "webpack --mode development --module-bind js=babel-loader ./docs/demo/main.js --output ./docs/demo/bundle.js",
    "watch:server": "node task/browsersync.js development",
    "server": "node task/browsersync.js production",
    "doc": "npm run doc:generate && npm run doc:replaceNode",
    "doc:generate": "typedoc --tsconfig tsdocconfig.json src",
    "doc:replaceNode": "node ./task/replaceAPI.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  }
}
